{
	"version": 2,
	"env": {
		"SUPABASE_URL": "@supabase_url",
		"SUPABASE_KEY": "@supabase_key"
	},
	"builds": [
		{
			"src": "index.ts",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["dist/**/*", "pathConfig.js"]
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/index.js"
		}
	]
}
